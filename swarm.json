{
  "app_name": "currentweather-app",
  "services": [
    {
      "service_name": "currentweather-service",
      "components": [
        {
          "component_name": "nodejs",
          "image": "registry.giantswarm.io/$company/currentweather",
          "ports": ["1337/tcp"],
          "dependencies": [
            {
              "name": "redis",
              "port": 6379
            }
          ],
          "domains": {
            "currentweather-$company.gigantic.io": "1337"
          }
        },
        {
          "component_name": "redis",
          "image": "redis",
          "ports": ["6379/tcp"]
        }
      ]
    }
  ]
}